---
    import {skills} from '@cv'
---

<section class="flex border-t pt-10 w-full justify-between gap-10 mb-20">
    <h2 class="uppercase">Competencias</h2>
    <div class="flex items-center gap-4 flex-wrap skills w-[31.5rem]">
        {skills.map((s) => (
            <p class="bg-gray-100 px-4 py-1 rounded-md text-gray-500">
                {s.skill}
            </p>
        ))}
    </div>
</section>